homeassistant:
  # Name of the location where Home Assistant is running
  name: Home
  # Location required to calculate the time the sun rises and sets
  latitude: 12.9716
  longitude: 77.5937
  # Impacts weather/sunrise data (altitude above sea level in meters)
  elevation: 920
  # metric for Metric, imperial for Imperial
  unit_system: metric
  # Pick yours from here: http://en.wikipedia.org/wiki/List_of_tz_database_time_zones
  time_zone: "{{ timezone }}"
  currency: INR
  country: IND
  external_url: "https://homeassistant.homelab.uknth.me"
  internal_url: "http://homeassistant.homelab.hero"
  auth_providers:
    - type: homeassistant
  media_dirs:
    media: "/data/homeassistant/media"
    recordings: "/data/homeassistant/recordings"